@{
    ViewBag.Title = "Kategori Oluştur";
}


<div class="row">
    <div class="col-lg-12 animatedParent animateOnce z-index-50">
        <div class="panel panel-default animated fadeInUp">
            <div class="panel-heading clearfix">
                <h3 class="panel-title">Kategori Oluştur</h3>
                <ul class="panel-tool-options">
                    <li><a data-rel="collapse" href="#"><i class="icon-down-open"></i></a></li>
                    <li><a data-rel="reload" href="#"><i class="icon-arrows-ccw"></i></a></li>
                    <li><a data-rel="close" href="#"><i class="icon-cancel"></i></a></li>
                </ul>
            </div>
            <div class="panel-body">
                <form action="@Url.Action("InsertCategory","News")" method="post">
                    <div class="form-group">
                        <label for="emailaddress">Kullanıcı Adı</label>
                        <input type="text" class="form-control" name="category" placeholder="KAtegori adı..." />
                    </div>




                    <button type="submit" class="btn btn-primary">Gönder</button>

                </form>
            </div>
        </div>
    </div>

</div>

@section footerjs{


    <script src="@Url.Content("/js/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("/js/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
}
